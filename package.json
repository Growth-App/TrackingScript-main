{
  "name": "trackingscript-main",
  "version": "1.0.0",
  "description": "TypeScript version for the tracking scripts ",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "webpack --mode development --config webpack.config.mjs",
    "run:wp": "docker compose -p growthapp-wp up -d",
    "run:wp:down": "docker compose -p growthapp-wp down",
    "build:prod": "webpack --mode production --config webpack.config.mjs",
    "start": "webpack --mode development --config webpack.config.mjs --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Growth-App/TrackingScript-main.git"
  },
  "keywords": [
    "tracking",
    "scripts",
    "cro",
    "seo",
    "contents",
    "conversion",
    "rate"
  ],
  "author": "GrowthApp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Growth-App/TrackingScript-main/issues"
  },
  "homepage": "https://github.com/Growth-App/TrackingScript-main#readme",
  "devDependencies": {
    "@types/node": "^20.14.8",
    "@types/uuid": "^10.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "dotenv": "^16.4.5",
    "dotenv-webpack": "^8.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.2",
    "user-agent-data-types": "^0.4.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-obfuscator": "^3.5.1"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  }
}
